#nop ###############  GMCP  MAP INTEGRATION #####################

#EVENT {IAC SB GMCP room.info IAC SE}{
	#var quickbind_alias_red $quickbind_alias_red_dimmed;
	#nop var quickbind_alias_blue $quickbind_alias_blue_dimmed;
	#var quickbind_alias_yellow $quickbind_alias_yellow_dimmed;
	#var quickbind_alias_green $quickbind_alias_green_dimmed;

	#if { "$lastmove" != "none" } {
		#chat message mapper navigate:exit:$lastmove;
		#var lastmove none;
	};
	
	
	#var ROOM_INFO {%0};

	#var pr_n {<G03> <088>};
	#var pr_s {<G03> <088>};
	#var pr_e {<G03> <088>};
	#var pr_w {<G03> <088>};
	#var pr_ne {<G03> <088>};
	#var pr_se {<G03> <088>};
	#var pr_sw {<G03> <088>};
	#var pr_nw {<G03> <088>};
	#var pr_d {<AAA>  <088>};
	#var pr_u {<AAA>  <088>};

	
	#list ROOM_INFO[exits] size rinfosize;
	
	#if {$rinfosize > 0} {
	
	#foreach {$ROOM_INFO[exits][]} {rinfo} {
		#var nxtexit $ROOM_INFO[exits][$rinfo];
		#if {"$nxtexit" == "polnoc"} {#var pr_n $GBLOCK;};
		#elseif {"$nxtexit" == "zachod"} {#var pr_w $GBLOCK;};
		#elseif {"$nxtexit" == "wschod"} {#var pr_e $GBLOCK;};
		#elseif {"$nxtexit" == "poludnie"} {#var pr_s $GBLOCK;};
		#elseif {"$nxtexit" == "dol"} {#var pr_d $GBLOCKD;};
		#elseif {"$nxtexit" == "gora"} {#var pr_u $GBLOCKU;};
		#elseif {"$nxtexit" == "polnocny-zachod"} {#var pr_nw $GBLOCK;};
		#elseif {"$nxtexit" == "polnocny-wschod"} {#var pr_ne $GBLOCK;};
		#elseif {"$nxtexit" == "poludniowy-zachod"} {#var pr_sw $GBLOCK;};
		#elseif {"$nxtexit" == "poludniowy-wschod"} {#var pr_se $GBLOCK;};
		#else {
		   #format {nxtexitlen} {%L} {$nxtexit};
		   #if {$nxtexitlen > 0} {
		   #line substitute {variables} {#alias {$nxtexit} {#send $nxtexit;#var lastmove $nxtexit}};
		   #nop sh $nxtexit;
		   };
		};
	};
        };
	#var ruch_kompasu {none};
	#var kompas_dziala {0};
	testprompt;
	
}

#var isNstandard 1
#var isEstandard 1
#var isWstandard 1
#var isSstandard 1
#var isNEstandard 1
#var isNWstandard 1
#var isSEstandard 1
#var isSWstandard 1
#var isUstandard 1
#var isDstandard 1

#action {<CHAT> mapper chats to everyone, 'room:id:%1'} {#var current_room_id %1;#line gag;}
#action {<CHAT> mapper chats to everyone, 'exit:end'} {testprompt;#line gag;}
#action {<CHAT> mapper chats to everyone, 'exit:start'} {#line gag;}
#action {<CHAT> You chat to %1, %2} {#line gag}


#action {^<CHAT> mapper chats to everyone, 'room:enter'} {
	#line gag;
	  #var isSstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_S} {move_s}};
	  #alias s {move_s};
	  #var isNstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_N} {move_n}};
	  #alias n {move_n};
	  #var isWstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_W} {move_w}};
	  #alias w {move_w};
	  #var isEstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_E} {move_e}};
	  #alias e {move_e};
	  #var isNWstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_NW} {move_nw}};
	  #alias nw {move_nw};
	  #var isNEstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_NE} {move_ne}};
	  #alias ne {move_ne};
	  #var isSWstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_SW} {move_sw}};
	  #alias sw {move_sw};
	  #var isSEstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_SE} {move_se}};
	  #alias se {move_se};
	  #var isUstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_U} {move_u}};
	  #alias u {move_u};
	  #var isDstandard 1;
	  #line substitute {variables} {#macro {$KEY_PAD_D} {move_d}}
	  #alias d {move_d};
}

