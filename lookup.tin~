#nop save lookup
#alias {save_lookup}
{
	#format rl {%L} {$current_room_id};
	#format nl {%L} {$ROOM_INFO[map][name]};
	#if {$nl > 0} {
		#if {$rl > 0} {
#var lookups[$ROOM_INFO[map][name]][$ROOM_INFO[map][x]][$ROOM_INFO[map][y]] {$current_room_id};
#system {echo "#var lookups[$ROOM_INFO[map][name]][$ROOM_INFO[map][x]][$ROOM_INFO[map][y]] {$current_room_id};" >> /home/adam/arkatt/lookup_data.tin};
			
		}
	} {
		#sh {brak gmcp na tej lokacji....};
	}
}

#alias {check_lookup}
{
	#format nl {%L} {$ROOM_INFO[map][name]};
	#if {$nl > 0} {
		#var lookup_room_id {$lookups[$ROOM_INFO[map][name]][$ROOM_INFO[map][x]][$ROOM_INFO[map][y]]};
		#if {$current_room_id <> $lookup_room_id} {
			#sh {gmcp resync};
			#chat message mapper lookup:$lookup_room_id;	
		};
	};
}

#system {echo "#var lookups[$ROOM_INFO[map][name]][$ROOM_INFO[map][x]][$ROOM_INFO[map][y]] {$current_room_id};" >> /home/adam/arkatt/lookup_data.tin};
