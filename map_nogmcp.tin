	#var pr_d { };
	#var pr_u { };
	
	#list ROOM_INFO[exits] size rinfosize;
	#format mnamelen {%L} {$ROOM_INFO[map][name]};
	#if {$rinfosize > 0} {
	
	#foreach {$ROOM_INFO[exits][]} {rinfo} {
		#var nxtexit $ROOM_INFO[exits][$rinfo];
		#if {"$nxtexit" == "polnoc"} {#var pr_n {|}};
		#elseif {"$nxtexit" == "zachod"} {#var pr_w {-}};
		#elseif {"$nxtexit" == "wschod"} {#var pr_e {-}};
		#elseif {"$nxtexit" == "poludnie"} {#var pr_s {|}};
		#elseif {"$nxtexit" == "dol"} {#var pr_d {D}};
		#elseif {"$nxtexit" == "gora"} {#var pr_u {U}};
		#elseif {"$nxtexit" == "polnocny-zachod"} {#var pr_nw {\\}};
		#elseif {"$nxtexit" == "polnocny-wschod"} {#var pr_ne {/}};
		#elseif {"$nxtexit" == "poludniowy-zachod"} {#var pr_sw {/}};
		#elseif {"$nxtexit" == "poludniowy-wschod"} {#var pr_se {\\}};
		#else {
		   #format {nxtexitlen} {%L} {$nxtexit};
		   #if {$nxtexitlen > 0} {
		   #line substitute {variables} {#alias {$nxtexit} {#send $nxtexit;#chat message mapper navigate:exit:$nxtexit}};
		   #nop sh $nxtexit;
		   };
		};
	};

		#if {$mnamelen > 0} {
			#replace {ROOM_INFO[map][name]} { } {};
			#replace {ROOM_INFO[map][name]} {'} {};
			#nop asciimap;
		};
			#nop asciicompas;
	};
	team_clean_compare;
	#var ruch_kompasu {none};
	#var kompas_dziala {0};
#sh prompt7;
#sh prompt6;
#sh prompt5;
#sh prompt3;
#sh prompt2;
#sh prompt1;
}

