#alias {asciicompas}
{
#if {$mnamelen <= 0} {
	#var asciimap_h 11;
	#var asciimap_w 40;
	#unvar asciimapv;
	#format wpadder {%+${asciimap_w}s} {};
	#loop 1 $asciimap_h loopcnt {
		#var asciimapv[$loopcnt] {$wpadder};
	};
	#var asciimapv[1] {                                        };
	#var asciimapv[2] {         $pr_nw          $pr_n          $pr_ne        };
	#var asciimapv[3] {           $pr_nw        $pr_n        $pr_ne          };
	#var asciimapv[4] {             $pr_nw      $pr_n      $pr_ne            };
	#var asciimapv[5] {                                        };
	#var asciimapv[6] {         $pr_w $pr_w $pr_w      <ffa>#<088>      $pr_e $pr_e $pr_e        };
	#var asciimapv[7] {                                        };
	#var asciimapv[8] {             $pr_sw      $pr_s      $pr_se            };
	#var asciimapv[9] {           $pr_sw        $pr_s        $pr_se          };
	#var asciimapv[10] {         $pr_sw          $pr_s          $pr_se        };
	#var asciimapv[11] {                                        };
};
};

#alias {asciimap}
{
	#var asciimap_h 11;
	#var asciimap_w 40;
	#unvar asciimapv;
	#format wpadder {%+${asciimap_w}s} {};
	#loop 1 $asciimap_h loopcnt {
		#var asciimapv[$loopcnt] {$wpadder};
	};
	#nop system {/Users/eldakar/arkatt/maps/showmap /Users/eldakar/arkatt/maps/$ROOM_INFO[map][name].map $ROOM_INFO[map][x] $ROOM_INFO[map][y] $asciimap_w 12};	
	#script {asciimapv} {$path/maps/showmap $path/maps/$ROOM_INFO[map][name].map $ROOM_INFO[map][x] $ROOM_INFO[map][y] $asciimap_w 12};
	#loop 1 $asciimap_h loopcnt {
		#var tmpline {$asciimapv[$loopcnt]};
		#format {tmpline} {%-${asciimap_w}s} {$tmpline};
		#replace {tmpline} {\\} {\\\};
		#replace {tmpline} {G} {<faf>G<088>};		
		#replace {tmpline} {#} {<faa>#<088>};
		#replace {tmpline} {P} {<afa>P<088>};		
		#replace {tmpline} {S} {<ffa>S<088>};		
		#replace {tmpline} {K} {<afa>K<088>};		
		#replace {tmpline} {L} {<acf>L<088>};		
		#replace {tmpline} {T} {<fff>T<088>};		
		#replace {tmpline} {N} {<aaf>N<088>};		
		#replace {tmpline} {F} {<acf>F<088>};
		#replace {tmpline} {B} {<ffa>B<088>};
		#var asciimapv[$loopcnt] {$tmpline};
		#nop sh $asciimapv[$loopcnt];
	}
}

