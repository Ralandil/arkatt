#chat init 7777;
#chat name wparty;
#split 0 22;

#loop {2} {21} {lc} {#line substitute {variables} {#prompt {line${lc}} {$line$lc} {$lc}};};

#loop {2} {21} {lc} {
	#line substitute {variables} {
		#action {~<CHAT> %* chats to you, 'line$lc%*'} {
			#var line${lc} {%2};
			#sh {line${lc}};
			#line gag;
		};
	};
};

#action {~<CHAT> %* chats to you, 'chat%*'} {
	#sh {%2};
	#line gag;
};